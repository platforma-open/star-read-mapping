{
  "name": "@milaboratory/milaboratories.star-read-mapping",
  "version": "1.0.2",
  "scripts": {
    "build": "rm -rf ./dist && block-tools pack",
    "build-all": "pnpm run -r build"
  },
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
